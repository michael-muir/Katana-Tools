t;ops.Grover.opArgs.RootWidth&quot;,&#0010;            FloatAttribute(newRootAttr))&#0010;    end&#0010;    ----- Mid&#0010;    if (midAttr ~= nil) then&#0010;        local newMidAttr = midAttr:getValue() * thicknessMult&#0010;        Interface.SetAttr(&#0010;            &quot;ops.Grover.opArgs.MidWidth&quot;,&#0010;            FloatAttribute(newMidAttr))&#0010;    end&#0010;    ----- Tip&#0010;    if (tipAttr ~= nil) then&#0010;        local newTipAttr = tipAttr:getValue() * thicknessMult&#0010;        Interface.SetAttr(&#0010;            &quot;ops.Grover.opArgs.TipWidth&quot;,&#0010;            FloatAttribute(newTipAttr))&#0010;    end&#0010;    ----- Density&#0010;    if (densityAttr ~= nil) then&#0010;        local newDensityAttr =densityAttr:getValue() * densityMult&#0010;        Interface.SetAttr(&#0010;            &quot;ops.Grover.opArgs.Density&quot;,&#0010;            FloatAttribute(newDensityAttr))&#0010;    end&#0010;end&#0010;searchChildren(searchPath)"/>
