          <group_parameter name="OPS_cameraCulling29">
            <string_parameter expression="=^/user.CEL" name="CEL"/>
            <string_parameter name="location" value="/root/world/location"/>
            <group_parameter name="script">
              <string_parameter name="lua" value="-- Read OpArgs for camera settings&#0010;local camPath = Interface.GetOpArg(&quot;user.camera.path&quot;):getValue()&#0010;local getResAttr = Interface.GetOpArg(&quot;user.camera.res&quot;):getNearestSample(0)[1]&#0010;local adjustWindow = Interface.GetOpArg(&quot;user.camera.adjustScreenWindow&quot;):getValue()&#0010;local frustumOffset = Interface.GetOpArg(&quot;user.camera.frustumOffset&quot;):getValue()&#0010;local overscanPercentX = Interface.GetOpArg(&quot;user.camera.overscanPercentX&quot;):getValue()&#0010;local overscanPercentY = Interface.GetOpArg(&quot;user.camera.overscanPercentY&quot;):getValue()&#0010;local distMG = Interface.GetOpArg(&quot;user.distanceMG&quot;):getValue()&#0010;local distBG = Interface.GetOpArg(&quot;user.distanceBG&quot;):getValue()&#0010;local excludeCEL = Interface.GetOpArg(&quot;user.excludeObjects&quot;):getValue()&#0010;local excludeObjects = {}&#0010;&#0010;if excludeCEL ~= nil then&#0010;    excludeCEL = excludeCEL:match(&quot;^%((.*)%)$&quot;)  -- Remove parentheses&#0010;    if excludeCEL ~= nil then&#0010;        for path in excludeCEL:gmatch(&quot;%S+&quot;) do  -- Split by space and store in a table&#0010;            table.insert(excludeObjects, path)&#0010;        end&#0010;    end&#0010;end&#0010;&#0010;-- Resolution â†’ aspect ratio&#0010;local resTable = ResolutionTable.GetResolution(getResAttr)&#0010;local aspectRatio = resTable:getAspectRatio()&#0010;&#0010;-- Camera FOV and clipping&#0010;local fovDegrees = Interface.GetAttr(&apos;geometry.fov&apos;, camPath):getValue()&#0010;local nearClip = Interface.GetAttr(&apos;geometry.near&apos;, camPath):getValue()&#0010;local farClip = Interface.GetAttr(&apos;geometry.far&apos;, camPath):getValue()&#0010;&#0010;-- Convert vertical FOV to radians&#0010;local fovy = fovDegrees * math.pi / 180 + frustumOffset&#0010;&#0010;-- Derive horizontal FOV from aspect ratio&#0010;local fovx = 2 * math.atan(math.tan(fovy / 2) * aspectRatio)&#0010;&#0010;-- Swap FOVs if adjusting width&#0010;if adjustWindow == 1 then&#0010;    fovx, fovy = fovy, fovx&#0010;end&#0010;&#0010;-- Overscan-adjusted FOVs for culling only&#0010;local fovxCull = fovx * (1 + (overscanPercentX / 100))&#0010;local fovyCull = fovy * (1 + (overscanPercentY / 100))&#0010;&#0010;-- Camera matrix and position&#0010;local camMatrix = XFormUtils.CalcTransformMatrixAtTime(Interface.GetGlobalXFormGroup(camPath), 0.0):getNearestSample(0.0)&#0010;local camM44 = Imath.M44d(camMatrix)&#0010;local camPos = camM44:translation()&#0010;&#0010;-- Frustum and axis&#0010;local frustum = Imath.Frustumd(nearClip, farClip, fovxCull, fovyCull, aspectRatio)&#0010;local camFrustum = Imath.FrustumTestd(frustum, camM44)&#0010;&#0010;-- Direction camera is facing&#0010;local camAxisRaw = Imath.V4d({0, 0, -1, 0}) * camM44&#0010;local camAxis = Imath.V3d({camAxisRaw.x, camAxisRaw.y, camAxisRaw.z}):normalized()&#0010;&#0010;-- Get current location&#0010;local loc = Interface.GetInputLocationPath()&#0010;local locType = Interface.GetAttr(&quot;type&quot;, loc):getValue()&#0010;local exclude = false&#0010;-- Culling only applies to geometry&#0010;if locType == &quot;usd point instancer&quot; or locType == &quot;component&quot; or locType == &quot;subdmesh&quot; or locType == &quot;polymesh&quot; then&#0010;    for i, path in ipairs(excludeObjects) do  -- Example: print the results&#0010;        if string.find(loc, path) then&#0010;            -- print(&quot;omit&quot;, loc, path)&#0010;            exclude = true&#0010;            break&#0010;        end&#0010;    end&#0010;&#0010;    if not exclude then&#0010;        -- print(&quot;not omit&quot;)&#0010;        local bounds = Interface.GetAttr(&apos;bound&apos;, loc)&#0010;        if bounds then&#0010;            local bounds = bounds:getNearestSample(0)&#0010;            local bbMin = Imath.V3d(bounds[1], bounds[3], bounds[5])&#0010;            local bbMax = Imath.V3d(bounds[2], bounds[4], bounds[6])&#0010;            local meshXformAttr = XFormUtils.CalcTransformMatrixAtTime(Interface.GetGlobalXFormGroup(loc), 0.0)&#0010;            local meshXformData = meshXformAttr:getNearestSample(0)&#0010;            local bbBox = Imath.Box3d(bbMin, bbMax):transform(Imath.M44d(meshXformData))&#0010;&#0010;            local isVisible = camFrustum:isVisible(bbBox)&#0010;            -- print(&quot;++++++++++++++++++++++++++++++++++++&quot;)&#0010;            -- print(&quot;Checking location: &quot; .. loc)&#0010;            -- print(&quot;Camera position: &quot; .. tostring(camPos))&#0010;            -- print(&quot;Camera direction: &quot; .. tostring(camAxis))&#0010;&#0010;            -- print(&quot;Transformed bounding box min: &quot; .. tostring(bbBox.min))&#0010;            -- print(&quot;Transformed bounding box max: &quot; .. tostring(bbBox.max))&#0010;&#0010;            local bbCenter = (bbBox.min + bbBox.max) / 2&#0010;            local toCenter = (bbCenter - camPos):normalized()&#0010;            local dot = camAxis:dot(toCenter)&#0010;            &#0010;            -- print(&quot;Bounding box center: &quot; .. tostring(bbCenter))&#0010;            -- print(&quot;Camera to box dot product: &quot; .. dot)&#0010;            &#0010;            local dist = (bbCenter - camPos):length()&#0010;            -- print(&quot;Distance from camera: &quot; .. dist)&#0010;&#0010;            if dist &lt; distMG then&#0010;                Interface.SetAttr(&apos;frustum.distance&apos;, StringAttribute(&quot;fg&quot;))&#0010;                -- print(&quot;distance &quot;.. dist .. &quot; is less than &quot; ..distMG)&#0010;            elseif distMG &lt;= dist and dist &lt; distBG then&#0010;                Interface.SetAttr(&apos;frustum.distance&apos;, StringAttribute(&quot;mg&quot;))&#0010;                -- print(&quot;distance &quot;.. dist .. &quot; is greater than &quot; ..distMG..&quot; and less than &quot; ..distBG)&#0010;            else&#0010;                Interface.SetAttr(&apos;frustum.distance&apos;, StringAttribute(&quot;bg&quot;))&#0010;                -- print(&quot;distance &quot;.. dist .. &quot; will be bg&quot;)&#0010;            end&#0010;            &#0010;        else&#0010;            Interface.SetAttr(&apos;frustum.distance&apos;, StringAttribute(&quot;bg&quot;))&#0010;            -- print(&quot;No bbox found will be part of the bg&quot;)&#0010;        end&#0010;    end&#0010;end&#0010;"/>
            </group_parameter>
