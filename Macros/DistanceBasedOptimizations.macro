                <number_parameter expression="=^/user.cameraSettings.frustumOffset" hints="{&apos;slider&apos;: &apos;True&apos;, &apos;slidercenter&apos;: &apos;0.0&apos;, &apos;slidermin&apos;: &apos;-1.0&apos;, &apos;constant&apos;: &apos;True&apos;}" isexpression="true" name="frustumOffset"/>
                <number_parameter expression="=^/user.cameraSettings.adjustScreenWindow" hints="{&apos;widget&apos;: &apos;mapper&apos;, &apos;options&apos;: {&apos;No adjustment&apos;: 0.0, &apos;Adjust height to match resolution&apos;: 1.0, &apos;Adjust width to match resolution&apos;: 2.0}, &apos;options__order&apos;: [&apos;No adjustment&apos;, &apos;Adjust height to match resolution&apos;, &apos;Adjust width to match resolution&apos;], &apos;constant&apos;: &apos;True&apos;}" isexpression="true" name="adjustScreenWindow"/>
