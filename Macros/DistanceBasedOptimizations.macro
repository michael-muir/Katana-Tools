          <string_parameter hints="{&apos;widget&apos;: &apos;scriptButton&apos;, &apos;buttonText&apos;: &apos;Calculate&apos;, &apos;scriptText&apos;: &apos;from Katana import NodegraphAPI, FnGeolib, Nodes3DAPI\n\nnode.getParameter(\&apos;user.mode\&apos;).setValue(1, 0)\n# --- Configuration ---\nFRUSTUM_LEVELS = [&quot;dummy&quot;, &quot;fg&quot;, &quot;mg&quot;, &quot;bg&quot;]   # dummy level fixes a delayed scene graph update\nSEARCH_PATH = \&apos;/root/world/geo/world\&apos;\nexclude_objects = node.getParameter(\&apos;user.excludeObjects\&apos;).getValue(0)\n\n# Create Client to call attributes from sceneGraph\nruntime = FnGeolib.GetRegisteredRuntimeInstance()       # We first need to get a hold of the runtime the UI is using\ntxn = runtime.createTransaction()                       # Transactions are used to batch together actions\nclient = txn.createClient()                             # Make a client, and attach it to an Op in the tree, we get the Op from a reference to a Node.\nop = Nodes3DAPI.GetOp(txn, node)\ntxn.setClientOp(client, op)\nruntime.commit(txn)                                     # Commit the transaction to the runtime, if we don\&apos;t the client will exist, but won\&apos;t yet be pointing to an Op\n\n# --- CEL builder ---\ndef build_cel(distance, include_char=True, include_prop=True, include_char_inst=True):\n    parts = []\n    if include_char:\n        parts.append(f\&apos;/root/world/geo/world/char//*{{@frustum.distance == &quot;{distance}&quot;}}\&apos;)\n    if include_prop:\n        parts.append(f\&apos;/root/world/geo/world/prop//*{{@frustum.distance == &quot;{distance}&quot;}}\&apos;)\n    if include_char_inst:\n        parts.append(f\&apos;/root/world/geo/world/char_inst//*{{@frustum.distance == &quot;{distance}&quot;}}\&apos;)\n    return \&apos; \&apos;.join(parts)\n\n# --- Path extractor ---\ndef extract_component_paths(paths, skip_keywords=(&quot;char_inst&quot;,), component_keywords=(&quot;char&quot;, &quot;prop&quot;)):\n    seen = set()\n    components = []\n\n    for path in paths:\n        if any(f\&apos;/{skip}/\&apos; in path for skip in skip_keywords):\n            if path not in seen:\n                seen.add(path)\n                components.append(path)\n            continue\n\n        for keyword in component_keywords:\n            split_key = f\&apos;/{keyword}/\&apos;\n            if split_key in path:\n                before, after = path.split(split_key, 1)\n                if after:\n                    component_name = after.split(\&apos;/\&apos;)[0]\n                    component_path = f&quot;{before}/{keyword}/{component_name}&quot;\n                    if component_path not in seen and component_path not in exclude_objects:\n                        seen.add(component_path)\n                        components.append(component_path)\n                break\n    # print(f&quot;[extract_component_paths] Final components: {components}&quot;)\n    return components\n\n# --- Frustum path collector ---\ndef collect_all_frustum_component_paths(node, search_path):\n    raw_results = {}\n    final_results = {}\n\n    # First collect everything raw\n    for distance in FRUSTUM_LEVELS:\n        cel = build_cel(distance)\n        collector = Widgets.CollectAndSelectInScenegraph(cel, search_path)\n        all_matched_paths = collector.collectAndSelect(select=False, node=node)        \n        final_paths = extract_component_paths(all_matched_paths)\n        raw_results[distance] = final_paths\n\n    # Then filter to make objects exclusive to the highest-priority level they appear in\n    seen = set()\n    for distance in FRUSTUM_LEVELS:\n        exclusive_paths = []\n        for path in raw_results[distance]:\n            if path not in seen:\n                seen.add(path)\n                exclusive_paths.append(path)\n        final_results[distance] = exclusive_paths\n\n    return final_results\n\n# --- Update collection nodes ---\ndef update_collection_nodes(node, frustum_paths):\n    for child in node.getChildren():\n        if child.getType() != \&apos;CollectionCreate\&apos;:\n            continue\n\n        collection_name = child.getName()\n\n        for level in FRUSTUM_LEVELS:\n            expected_substring = f\&apos;CC_char_inst_{level}\&apos;\n            if expected_substring in collection_name:\n                cel_string = \&apos; \&apos;.join(frustum_paths[level])\n                # print(f&quot;[update_collection_nodes] Setting CEL for {collection_name}: {\&apos; \&apos;.join(frustum_paths[level])}&quot;)\n                child.getParameter(\&apos;CEL\&apos;).setValue(cel_string, 0)\n                break\n\n# --- Execute ---\nall_frustum_component_paths = collect_all_frustum_component_paths(node, SEARCH_PATH)\nupdate_collection_nodes(node, all_frustum_component_paths)\n\n\nnode.getParameter(\&apos;user.mode\&apos;).setValue(0, 0)\n&apos;}" name="calculate" value=""/>
